page {
  background-color: #F9F9F9;
}
.img {
  width: 100%;
  height: 100%;
}
.page {
  background-color: #F9F9F9;
}
.page .proBox {
  background-color: #FFFFFF;
  border-radius: 25rpx;
  overflow: hidden;
}
.page .proBox .boxHead {
  height: 220rpx;
  display: flex;
  padding: 15rpx;
}
.page .proBox .boxHead .headImg {
  width: 40%;
  height: 100%;
  border-radius: 25rpx;
  overflow: hidden;
}
.page .proBox .boxHead .headInfo {
  width: 60%;
  height: 100%;
  padding-left: 15rpx;
}
.page .proBox .boxHead .headInfo .infoTitle {
  line-height: 60rpx;
  font-size: 38rpx;
  margin-bottom: 5rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.page .proBox .boxHead .headInfo .infoOther {
  line-height: 50rpx;
  font-size: 29rpx;
  display: flex;
}
.page .proBox .boxBody {
  padding: 0 25rpx 20rpx;
}
.page .proBox .boxBody .bodyBox {
  line-height: 50rpx;
  font-size: 27rpx;
  display: flex;
  justify-content: space-between;
}
.page .proBox .boxBody .bodyBox .introduce {
  width: 400rpx;
  height: 120rpx;
  border: 1px #cfcfcf solid;
}
.page .checkBox {
  margin-top: 20rpx;
}
.page .checkBox .infoBox {
  padding: 10rpx;
  padding-left: 20rpx;
  background-color: #FFFFFF;
  border-radius: 25rpx;
  overflow: hidden;
}
.page .checkBox .infoBox .temp {
  margin-bottom: 20rpx;
}
.page .checkBox .infoBox .infoItem {
  padding: 0 10rpx 10rpx 40rpx;
  line-height: 50rpx;
  font-size: 27rpx;
  display: flex;
  justify-content: space-between;
}
.page .checkBox .infoBox .infoItem .resultBox {
  margin-right: 100rpx;
}
.page .checkBox .infoBox .infoItem .checkRemark {
  width: 400rpx;
  height: 100rpx;
  border: 1px #cfcfcf solid;
  color: #cfcfcf;
}
.page .checkBox .checkBtns {
  padding: 30rpx 0;
  display: flex;
}
.marqueeBox {
  background-color: #FFFFFF;
  padding: 20rpx 10rpx 0;
  border-radius: 25rpx 25rpx 0 0;
}
.marqueeBox button:not(:last-child) {
  background-color: #6CBA18;
  color: #fff;
}
.marqueeBox button:not(:first-child) {
  margin-top: 20rpx;
}

