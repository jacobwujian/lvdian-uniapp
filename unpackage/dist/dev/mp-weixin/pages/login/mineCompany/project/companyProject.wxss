page {
  background-color: #F9F9F9;
}
.search {
  width: 100%;
}
.uni-searchbar {
  padding: 5px;
}
.img {
  width: 100%;
  height: 100%;
}
.marqueeBox {
  background-color: #FFFFFF;
  padding: 20rpx 10rpx 0;
  border-radius: 25rpx 25rpx 0 0;
}
.marqueeBox button:not(:first-child) {
  margin-top: 20rpx;
}
.page {
  background-color: #F9F9F9;
}
.page .projectBox .proItem {
  height: 245rpx;
  padding: 15rpx;
  display: flex;
  background-color: #FFFFFF;
  border-radius: 25rpx;
}
.page .projectBox .proItem:not(:first-child) {
  margin-top: 20rpx;
}
.page .projectBox .proItem .proImg {
  width: 290rpx;
  height: 240rpx;
  border-radius: 25rpx;
  overflow: hidden;
}
.page .projectBox .proItem .infoBox {
  margin-left: 10rpx;
  width: 340rpx;
  height: 240rpx;
  padding-left: 15rpx;
}
.page .projectBox .proItem .infoBox .title {
  line-height: 47rpx;
  font-size: 35rpx;
  font-weight: 400;
  margin-bottom: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.page .projectBox .proItem .infoBox .otherInfo {
  height: 35rpx;
  line-height: 35rpx;
  font-size: 25rpx;
  margin-top: 5rpx;
  color: #9C9492;
  display: flex;
  white-space: nowrap;
}
.page .projectBox .proItem .infoBox .otherInfo .companyNameTemp {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}
.bottomBar {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}

